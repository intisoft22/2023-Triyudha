<odoo>
    <data noupdate="1"> <!-- noupdate="1" means the data is not updated when the module is updated -->
        <!-- Access rights for Custom Pricelist History -->
        <record id="custom_pricelist_history_access" model="ir.model.access">
            <field name="name">custom.pricelist.history.access</field>
            <field name="model_id" ref="model_custom_pricelist_history"/>
            <field name="group_id" ref="base.group_user"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_create" eval="1"/>
            <field name="perm_unlink" eval="1"/>
        </record>

        <record id="model_custom_pricelist_history" model="ir.model">
            <field name="name">custom.pricelist.history</field>
            <field name="model">custom.pricelist.history</field>
        </record>
